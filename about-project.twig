{% include("partials/_header.twig") %}
<section class="hero is-dark is-fullheight-with-navbar parallax" style="background: image({{ page.image }})">
    <!-- Hero head: will stick at the top -->
    <div class="hero-head">
        {%  include("partials/_navigation.twig") %}
    </div>
    <!-- Hero content: will be in the middle -->
    <div class="hero-body">
        <div class="container has-text-centered">
            <h1 style="font-size: 3rem;" class="title">
                {{ page.title }}
            </h1>
            <h2 style="font-size: 1.5rem;" class="subtitle">
                {{ page.teaser }}
            </h2>
        </div>
    </div>
</section>
<section class="section">
    <div class="container-fluid">
        <div class="columns">
            <div class="column">
                <div class="tile is-ancestor">
                    <div class="tile is-parent">
                        <div class="tile is-child box">
                            {% setcontent welcome = "block/welcome" %}
                            <p style="font-size: 2rem;" class="title is-centered">
                                {{ welcome.title }}
                            </p>
                            <br>
                            {{ welcome.content }}
                        </div>
                    </div>
                    <div class="tile is-5 is-parent">
                        <div class="tile is-child box">
                            {% setcontent about = "block/about-me" %}
                            <p style="font-size: 2rem;" class="title is-centered">
                                {{ about.title }}
                            </p>
                            <br>
                            {{ about.content }}
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
